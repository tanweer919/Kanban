{
  "root": true,
  "globals": {
    "defineProps": "readonly"
  },
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "plugins": [],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  }
}
